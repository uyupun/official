{
  "name": "official",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:eslint": "next lint --max-warnings 0 .",
    "lint:prettier": "prettier --check .",
    "fix:eslint": "next lint --fix --max-warnings 0 .",
    "fix:prettier": "prettier --write .",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s public"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "yarn fix:eslint",
      "yarn fix:prettier"
    ]
  },
  "dependencies": {
    "next": "latest",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@storybook/addon-actions": "^6.5.0-alpha.10",
    "@storybook/addon-essentials": "^6.5.0-alpha.10",
    "@storybook/addon-links": "^6.5.0-alpha.10",
    "@storybook/builder-webpack5": "^6.5.0-alpha.10",
    "@storybook/manager-webpack5": "^6.5.0-alpha.10",
    "@storybook/react": "^6.5.0-alpha.10",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "babel-loader": "^8.2.3",
    "eslint": "<8.0.0",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-storybook": "^0.5.5",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.0",
    "typescript": "^4.2.4"
  }
}
