{
  "name": "official",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn pathpida && next build && next export",
    "start": "next start",
    "lint:eslint": "next lint --max-warnings 0 . --ignore-path .eslintignore --cache --cache-strategy content",
    "lint:prettier": "prettier --check . --ignore-path .prettierignore --cache --cache-strategy content",
    "lint:ls": "ls-lint",
    "fix:eslint": "next lint --fix --max-warnings 0 . --ignore-path .eslintignore --cache --cache-strategy content",
    "fix:prettier": "prettier --write . --ignore-path .prettierignore --cache --cache-strategy content",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s public",
    "pathpida": "pathpida --ignorePath .gitignore --output src/lib/ && prettier --write src/lib/",
    "icons": "svgr -d src/components/ui/Icons src/assets/icons",
    "images": "node scripts/generateImages.js"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@hookform/resolvers": "^2.9.11",
    "clsx": "^1.2.1",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.7",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@ls-lint/ls-lint": "^2.0.0",
    "@storybook/addon-a11y": "^6.5.15",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@svgr/cli": "^6.5.1",
    "@types/node": "18.15.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@vanilla-extract/babel-plugin": "^1.1.4",
    "@vanilla-extract/css": "^1.6.8",
    "@vanilla-extract/next-plugin": "^2.0.1",
    "@vanilla-extract/recipes": "^0.3.0",
    "@vanilla-extract/sprinkles": "^1.5.1",
    "babel-loader": "^8.2.3",
    "chromatic": "^6.4.3",
    "eslint": "^8.32.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-storybook": "^0.6.10",
    "git-cz": "^4.9.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.2",
    "modern-css-reset": "^1.4.0",
    "pathpida": "^0.20.1",
    "prettier": "^2.8.3",
    "sharp": "^0.32.1",
    "storybook-addon-next": "^1.7.1",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": "18.15.0"
  },
  "resolutions": {
    "@types/react": "18.0.28"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
